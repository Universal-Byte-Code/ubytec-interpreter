{
  "$schema": "https://raw.githubusercontent.com/dotnet/docfx/main/schemas/docfx.schema.json",
  "metadata": [
    {
      "src": [
        {
          "src": "..",
          "files": [
            "**/*.csproj"
          ]
        }
      ],
      "dest": "api",
      "namespaceLayout": "nested",
      "categoryLayout": "nested",
      "enumSortOrder": "declaringOrder",
      "includePrivateMembers": true,
      "includeExplicitInterfaceImplementations": true,
      "memberLayout": "separatePages"
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "**/*.{md,yml}"
        ],
        "exclude": [
          "_site/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "output": "_site",
    "template": [
      "default",
      "modern"
    ],
    "globalMetadata": {
      "_appName": "Ubytec",
      "_appTitle": "Ubytec Language Wiki",
      "_enableSearch": true,
      "_appLogoPath": "images/favicon-32x32.png",
      "_appFaviconPath": "images/favicon.ico",
      "_enableNewTab": true,
      "pdf": false
    },
    "xref": [
      "../xrefmap.yml",
      "https://github.com/dotnet/docfx/raw/main/.xrefmap.json"
    ]
  }
}
